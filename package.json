{
  "name": "mycityonlinebackend",
  "version": "1.0.0",
  "description": "backend for mycityOnlinebackend",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "test": "npm run test",
    "lint-fix": "eslint --fix .",
    "lint-detect": "npx eslint .",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "start": "nodemon server/server",
    "prettier": "prettier --write .",
    "start:admin": "nodemon server/admin-server"
  },
  "husky": {
    "hooks": {
      "applypatch-msg": "echo \"[Husky] applypatch-msg\"",
      "pre-applypatch": "echo \"[Husky] pre-applypatch\"",
      "post-applypatch": "echo \"[Husky] post-applypatch\"",
      "pre-commit": "prettier --write . && git add -A ."
    }
  },
  "pre-commit": [
    "lint-detect"
  ],
  "lint-staged": {
    "*": "prettier --write"
  },
  "_moduleAliases": {
    "logger": "server/utils/logger"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "^3.0.2",
    "pre-commit": "^1.2.2",
    "prettier": "^3.2.5"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "axios-retry": "^4.1.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "express": "^4.18.2",
    "express-http-context": "^1.2.4",
    "express-joi-validation": "^5.0.1",
    "google-auth-library": "^9.11.0",
    "http-status-codes": "^2.3.0",
    "husky": "^9.0.11",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "mongoose": "^8.4.5",
    "mysql2": "^3.9.7",
    "save": "^2.9.0",
    "sequelize": "^6.37.3",
    "uuid": "^9.0.1",
    "uuidv4": "^6.2.13",
    "winston": "^3.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Abhishek79201/myCityOnline.git"
  },
  "author": "mycityonlinebackend",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/Abhishek79201/myCityOnline.git"
  },
  "homepage": "https://github.com/Abhishek79201/myCityOnline#readme"
}
